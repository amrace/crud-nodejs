<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
      integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T"
      crossorigin="anonymous"
    />
    <title>EJS Bootcamp</title>
  </head>
  <body>
    <div class="container">
      <div class="row mt-3">
        <div class="col ml-6">
          
            <h3 >Add New Person To Bootcamp</h3>
           <a href="/contact/new" class="btn btn-outline-primary">Add</a>
           
        
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Id</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
                <th scope="col">phone</th>
                <th scope="col">feedback</th>
                <th scope="col">Action</th>
                <th scope="col">Action</th>
              </tr>
            </thead>
            
            <tbody id="table"></tbody>
            <tr <% allUser.forEach(user=>{ %> >
              
                <th scope="row"><%= user.id %></th>
                <td> <%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.email %></td>
                <td><%= user.phone %></td>
                <td><%= user.feedback%></td>
                <td><a href="contact/edit/<%=user.id%>"><button
                  type="button" class="btn btn-primary" data-toggle="modal"
                  data-target="#exampleModal">Edit</button></a></td>
                <td><a href="contact/delete/<%=user.id%>"><button
                  type="button" class="btn btn-danger">Delete</button></a></td>                            
            </tr <% }) %>>
          </table>
        </div>
      </div>
    </div>

    <script type="module" src="./index.js"></script>
  </body>
</html>
